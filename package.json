    "npm-run-all2": "7.0.1",
    "pinia": "2.2.8",
    "typescript": "5.7.2",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-tsc": "2.1.10"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.2.1",
